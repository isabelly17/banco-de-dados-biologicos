create a table
CREATE TABLE biologia (')'
  ano INT(22) PRIMARY KEY,
  eventos VARCHAR(pandemias) NOT NULL,
  localidade VARCHAR (mundialmente) NOT NULL,
descricao VARCHAR (vacinas e doenças ao passar dos anos) NOT NULL,
);
